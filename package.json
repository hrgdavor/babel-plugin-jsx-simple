  "unpkg": "dist/babel-plugin-jsx-simple.min.js",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "lint": "eslint index.js",
    "test": "npm run lint && mocha --compilers js:babel-register",
